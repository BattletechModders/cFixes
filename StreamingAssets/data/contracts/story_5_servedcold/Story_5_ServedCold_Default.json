        {
            "dialogue" : {
                "EncounterObjectGuid" : "f474f715-c387-4fff-9fe1-61154de07583"
            },
            "name" : "Inter_VictoriaLands",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "INCOMING TRANSMISSION.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_Computer",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "39045acd-b014-45d3-8bdd-6c0f3a91cb99",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : 120
                },
                {
                    "words" : "{COMMANDER.Callsign}. I remember you well\u2014the late Raju Montgomery held you in <i>such</i> high esteem. And now I find you here, serving my exiled cousin. Where is she hiding, I wonder?",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_VictoriaDefault",
                    "emote" : "Default",
                    "audioName" : "m050_100_victoria",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : 0
                },
                {
                    "words" : "Victoria. If I'd known that we'd find you here, I'd have taken the field myself.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_KameaDefault",
                    "emote" : "Default",
                    "audioName" : "m050_110_victoria",
                    "cameraFocusGuid" : "39045acd-b014-45d3-8bdd-6c0f3a91cb99",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : 0
                },
                {
                    "words" : "A pity you didn't\u2014we could have ended this war here and now. I suppose I'll have to content myself with your prized mercenaries. Watch as I tear them to shreds.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_VictoriaDefault",
                    "emote" : "Default",
                    "audioName" : "NONE",
                    "cameraFocusGuid" : "",
                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : -1
                }
            ]
        },
