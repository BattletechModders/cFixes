            "name" : "Inter_DropShipDown_VictoriaLands_b",
            "overrideDialogueBucketId" : "",
            "dialogueContent" : [
                {
                    "words" : "INCOMING TRANSMISSION.",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
