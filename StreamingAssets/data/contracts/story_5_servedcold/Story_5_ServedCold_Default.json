                    "cameraDistance" : "Far",
                    "cameraHeight" : "Default",
                    "revealRadius" : 0
                },
                {
                    "words" : "That must be the defense force Lord Karosas warned us about. Be ready.\r",
                    "wordsColor" : {
                        "r" : 1,
                        "g" : 1,
                        "b" : 1,
                        "a" : 1
                    },
                    "selectedCastDefId" : "castDef_MadeiraDefault",
